package com.demo;

import java.util.StringTokenizer;

public class WildCards {

	public static void main(String[] args) {
		String wc = "";
		String str = "";
	}

	public static boolean checkTokens(String wc, char[] str) {
		boolean found;
		for (int i=0 ; i< str.length ; i++) {
			found = true;
			for (int j=i ; j<str.length ; j++) {
				StringTokenizer tokenizer = new StringTokenizer(wc, "*");
				while (tokenizer.hasMoreTokens()) {
					if (!(tokenizer.equals(str.toString().substring(j, 
							(tokenizer.toString().length() - 1))))) {
						j=str.length;
						found = false;
						break;
					}
				}
				if (found) {
					return true;
				}
			}
		}
		return false;
	}

}
