package com.demo;

import java.util.StringTokenizer;

public class WildCards {

	public static void main(String[] args) {
		String wc = "";
		String str = "";
		StringTokenizer tokenizer = new StringTokenizer(wc, "*");
		while (tokenizer.hasMoreTokens()) {
			
		}
		checkEquivalence(wc.toCharArray(), 0, str.toCharArray(), 0);
	}

	private static boolean checkEquivalence(char[] wc, int wi, char[] str, int si) {

		if (si >= str.length)
			return false;
		
		return true;
	}

}
